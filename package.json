{
    "name": "gvb-tests",
    "description": "functional tests for our gulp-vsts-bump module",
    "version": "0.0.4",
    "private": true,
    "license": "MIT",
    "scripts": {
        "lint": "eslint **/*.js",
        "clean": "rimraf .testresults .testcontext",
        "pretest": "npm run clean && ncp test/fixtures/ .testcontext/",
        "test": "mocha --opts test/mocha.opts",
        "gulp": "cd .testcontext && gulp",
        "target:remove": "npm r gulp-vsts-bump",
        "pretarget:install": "npm run target:remove",
        "target:install": "npm i --no-save gulp-vsts-bump",
        "pretarget:install:file": "npm run target:remove",
        "target:install:file": "npm i --no-save"
    },
    "devDependencies": {
        "@swellaby/eslint-config": "^1.1.6",
        "chai": "^4.1.2",
        "eslint": "^5.1.0",
        "gulp": "^3.9.1",
        "mocha": "^5.2.0",
        "mocha-multi": "^1.0.1",
        "ncp": "^2.0.0",
        "rimraf": "^2.6.2",
        "shelljs": "^0.8.2"
    },
    "dependencies": {}
}
